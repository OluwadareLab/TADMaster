clustertad=False 

catch=False 

arrowhead=False 					
arrowhead_genomeID=hg19 

topdom=False 
topdom_window=5 

armatus=True
armatus_gamma=0.2 

di=False
di_length=10 

spectral=False

gmap=False

ic_finder=False 

chromo_r=False 

hic_seg=False 

insulation=False
insulation_window=0 

hic_explorer=False				 